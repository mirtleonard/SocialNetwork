<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="BOTTOM_CENTER" prefHeight="438.0" prefWidth="322.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" >
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="25.0" />
    </padding>
   <Label fx:id="labelEmail" prefHeight="18.0" prefWidth="303.0" text="Email" />
   <TextField fx:id="email" />
   <Label prefHeight="18.0" prefWidth="297.0" text="Password:" />
   <PasswordField fx:id="password" />
    <Button onAction="#login" prefWidth="282.4" text="Login" translateY="35.0">
      <VBox.margin>
         <Insets />
      </VBox.margin></Button>

          <Label fx:id="welcomeText" text=" Don't have an account?" translateY="25.0">
      <font>
         <Font size="11.0" />
      </font>
      <VBox.margin>
         <Insets />
      </VBox.margin>
   </Label>
   <Hyperlink text="Register" translateY="5.0" />
</VBox>
